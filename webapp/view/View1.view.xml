<mvc:View controllerName="ovly.random-dog.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Image width="100%" id="containner"   />
					</content>
					<footer>
						<Bar>
							<contentRight>
								<Button text="Get Dog" type="Emphasized" press="onGetRandomDog" />
							</contentRight>
						</Bar>
					</footer>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>